<h1>Create a new Campaign</h1>
<%= form_for @my_campaign do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :start_date %>
  <%= f.date_field :start_date, hide_label: true, class: 'datepicker' %>

  <%= f.label :end_date %>
  <%= f.date_field :end_date %>
  
  <%= f.button %>
<% end %>

