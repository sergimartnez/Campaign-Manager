<h1>Create a new Prototype</h1>
<%= form_for [@my_campaign, @my_prototype, @my_content] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_area :description %>
  
  <%= f.label :attachment %>
  <%= f.file_field :attachment %>

  <%= f.button %>
<% end %>

